<launch>

  <arg name="cmd_vel_out" default="cmd_vel"/>

  <arg name="config_topics" default="$(find evo_twist_mux)/config/twist_mux_topics.yaml"/>
  <arg name="config_locks"  default="$(find evo_twist_mux)/config/twist_mux_locks.yaml"/>

  <node pkg="twist_mux" type="twist_mux" name="twist_mux" output="screen">
    <remap from="cmd_vel_out" to="$(arg cmd_vel_out)"/>

    <!-- Twist mux takes in several Twist topics http://wiki.ros.org/twist_mux -->

    
    <rosparam file="$(arg config_topics)" command="load"/>
    <rosparam file="$(arg config_locks)"  command="load"/>
  </node>

</launch>

